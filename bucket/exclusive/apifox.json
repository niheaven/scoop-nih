{
    "version": "2.3.19",
    "description": "All-in-one collaboration platform for API documentation, API debugging, API Mock and API test automation.",
    "homepage": "https://apifox.com",
    "license": "Freeware",
    "architecture": {
        "64bit": {
            "url": "https://cdn.apifox.cn/download/2.3.19/Apifox-2.3.19.exe#/dl.7z",
            "hash": "sha512:77164f3c7bd0ebfee5fec618b4d302a5aa37148dce4a629c9d677fe4eef6c0c143ec264f42c2db3036f56b388a4cfbafbb5d8e7d2aa8c6353043bd47d077715e",
            "pre_install": [
                "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\"",
                "Remove-Item \"$dir\\`$*\", \"$dir\\Uninstall*\" -Force -Recurse"
            ]
        },
        "32bit": {
            "url": "https://cdn.apifox.cn/download/2.3.19/Apifox-windows-x86-2.3.19.exe#/dl.7z",
            "hash": "sha512:6084d14bb8ab054d08c5f9f18a4faf2cd597cfe9ffab24e8fec0935afaa744afca7e8f0f961d8323c117308d8f6e6ca1435fc72cf40df439f9e8b36a24758273",
            "pre_install": [
                "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-32.7z\" \"$dir\"",
                "Remove-Item \"$dir\\`$*\", \"$dir\\Uninstall*\" -Force -Recurse"
            ]
        }
    },
    "shortcuts": [
        [
            "Apifox.exe",
            "Apifox",
            "--user-data-dir=\"$dir\\UserData\""
        ]
    ],
    "persist": "UserData",
    "checkver": {
        "url": "https://cdn.apifox.cn/download/latest.yml",
        "regex": "version: ([\\d.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://cdn.apifox.cn/download/$version/Apifox-$version.exe#/dl.7z",
                "hash": {
                    "url": "https://cdn.apifox.cn/download/latest.yml",
                    "regex": "sha512: $base64"
                }
            },
            "32bit": {
                "url": "https://cdn.apifox.cn/download/$version/Apifox-windows-x86-$version.exe#/dl.7z",
                "hash": {
                    "url": "https://cdn.apifox.cn/download/x32/latest.yml",
                    "regex": "sha512: $base64"
                }
            }
        }
    }
}
