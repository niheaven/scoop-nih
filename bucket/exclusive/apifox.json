{
    "version": "2.3.16",
    "description": "All-in-one collaboration platform for API documentation, API debugging, API Mock and API test automation.",
    "homepage": "https://apifox.com",
    "license": "Freeware",
    "architecture": {
        "64bit": {
            "url": "https://cdn.apifox.cn/download/2.3.16/Apifox-2.3.16.exe#/dl.7z",
            "hash": "sha512:5fa745c0edb40fc64caab3a696dd5380a2bcf6da70b3a62d2c2f6b17f4017bee1bff0ff75ca84d3ac0ebc71afa3ed7e6c9f9bb21dc26b53098c8871f438576f4",
            "pre_install": [
                "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\"",
                "Remove-Item \"$dir\\`$*\", \"$dir\\Uninstall*\" -Force -Recurse"
            ]
        },
        "32bit": {
            "url": "https://cdn.apifox.cn/download/2.3.16/Apifox-windows-x86-2.3.16.exe#/dl.7z",
            "hash": "sha512:ce4a947383c96327972f69248fb4d6782625ac5b6da54bc89c88afdfabe06b2ab7b888e19415bb54e6d9639c8fb66e7274e78f464626a9c7f04182cc9ab56f1d",
            "pre_install": [
                "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-32.7z\" \"$dir\"",
                "Remove-Item \"$dir\\`$*\", \"$dir\\Uninstall*\" -Force -Recurse"
            ]
        }
    },
    "shortcuts": [
        [
            "Apifox.exe",
            "Apifox",
            "--user-data-dir=\"$dir\\UserData\""
        ]
    ],
    "persist": "UserData",
    "checkver": {
        "url": "https://cdn.apifox.cn/download/latest.yml",
        "regex": "version: ([\\d.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://cdn.apifox.cn/download/$version/Apifox-$version.exe#/dl.7z",
                "hash": {
                    "url": "https://cdn.apifox.cn/download/latest.yml",
                    "regex": "sha512: $base64"
                }
            },
            "32bit": {
                "url": "https://cdn.apifox.cn/download/$version/Apifox-windows-x86-$version.exe#/dl.7z",
                "hash": {
                    "url": "https://cdn.apifox.cn/download/x32/latest.yml",
                    "regex": "sha512: $base64"
                }
            }
        }
    }
}
