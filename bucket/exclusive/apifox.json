{
    "version": "2.3.10",
    "description": "All-in-one collaboration platform for API documentation, API debugging, API Mock and API test automation.",
    "homepage": "https://apifox.com",
    "license": "Freeware",
    "architecture": {
        "64bit": {
            "url": "https://cdn.apifox.cn/download/2.3.10/Apifox-2.3.10.exe#/dl.7z",
            "hash": "sha512:e555360491eab363e5e30451815359018f47f68746805743c1bfdb24dae6c50e8137bf1b89dc18958a10b39d30a27065e396050d48fc85fd15b5b52ebdb8ec5c",
            "pre_install": [
                "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\"",
                "Remove-Item \"$dir\\`$*\", \"$dir\\Uninstall*\" -Force -Recurse"
            ]
        },
        "32bit": {
            "url": "https://cdn.apifox.cn/download/2.3.10/Apifox-windows-x86-2.3.10.exe#/dl.7z",
            "hash": "sha512:3563f8e50c510d4457404192e0be1c90ca42a3725b6ad33c364afe0985cd3328d2528981dca313391baf3df6f2fd0bf69c803b79fb2eae6e5c94c59b96fd6bb3",
            "pre_install": [
                "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-32.7z\" \"$dir\"",
                "Remove-Item \"$dir\\`$*\", \"$dir\\Uninstall*\" -Force -Recurse"
            ]
        }
    },
    "shortcuts": [
        [
            "Apifox.exe",
            "Apifox",
            "--user-data-dir=\"$dir\\UserData\""
        ]
    ],
    "persist": "UserData",
    "checkver": {
        "url": "https://cdn.apifox.cn/download/latest.yml",
        "regex": "version: ([\\d.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://cdn.apifox.cn/download/$version/Apifox-$version.exe#/dl.7z",
                "hash": {
                    "url": "https://cdn.apifox.cn/download/latest.yml",
                    "regex": "sha512: $base64"
                }
            },
            "32bit": {
                "url": "https://cdn.apifox.cn/download/$version/Apifox-windows-x86-$version.exe#/dl.7z",
                "hash": {
                    "url": "https://cdn.apifox.cn/download/x32/latest.yml",
                    "regex": "sha512: $base64"
                }
            }
        }
    }
}
