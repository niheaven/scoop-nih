{
    "description": "Web browser that connects to the internet via the Tor anonymity network",
    "homepage": "https://www.torproject.org/",
    "version": "11.0.1",
    "##": "Multiple components under various open source licenses. Included HTTPS Everywhere extension is the strictest.",
    "license": "GPL-3.0-or-later",
    "architecture": {
        "64bit": {
            "url": "https://downloads.sourceforge.net/project/tor-browser.mirror/11.0.1/torbrowser-install-win64-11.0.1_zh-CN.exe",
            "hash": "sha1:d88c14405f45fcb7a427432dbf6cd158f93c7027"
        },
        "32bit": {
            "url": "https://downloads.sourceforge.net/project/tor-browser.mirror/11.0.1/torbrowser-install-11.0.1_zh-CN.exe",
            "hash": "sha1:066c90fd7a9b87ccf6a93e3ef55b5ad7c867025f"
        }
    },
    "installer": {
        "script": [
            "Invoke-ExternalCommand \"$dir\\$fname\" \"/S /D=$dir\" | Out-Null",
            "movedir \"$dir\\Browser\" \"$dir\" | Out-Null",
            "Remove-Item \"$dir\\$fname\", \"$dir\\*.lnk\" -Force"
        ]
    },
    "bin": [
        [
            "firefox.exe",
            "tor-browser"
        ]
    ],
    "shortcuts": [
        [
            "firefox.exe",
            "Tor Browser"
        ]
    ],
    "checkver": {
        "url": "https://sourceforge.net/projects/tor-browser.mirror/files/",
        "re": "files/([\\d.]+)/"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://downloads.sourceforge.net/project/tor-browser.mirror/$version/torbrowser-install-win64-$version_zh-CN.exe"
            },
            "32bit": {
                "url": "https://downloads.sourceforge.net/project/tor-browser.mirror/$version/torbrowser-install-$version_zh-CN.exe"
            }
        }
    }
}
